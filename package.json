{
  "name": "natours-review",
  "version": "0.0.1",
  "description": "A tours app developed on top of express ",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon server -e js,pug,css,env",
    "client:dev": "cross-env NODE_ENV=development webpack --mode development -w",
    "client:prod": "cross-env NODE_ENV=production webpack --mode production",
    "import": " node dev-data/data/load-data.js --import-all",
    "import:tours": "node dev-data/data/load-data.js --import-tours",
    "import:users": "node dev-data/data/load-data.js --import-users",
    "import:reviews": "node dev-data/data/load-data.js --import-reviews",
    "wipe": "node dev-data/data/load-data.js --wipe-all",
    "wipe:tours": "node dev-data/data/load-data.js --wipe-tours",
    "wipe:users": "node dev-data/data/load-data.js --wipe-users",
    "wipe:reviews": "node dev-data/data/load-data.js --wipe-reviews"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Marknjo/natours-api.git"
  },
  "keywords": [
    "Nodejs",
    "JavaScript",
    "Pug",
    "Html",
    "CSS",
    "Express"
  ],
  "author": "Mark Njoroge",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Marknjo/natours-api/issues"
  },
  "homepage": "https://github.com/Marknjo/natours-api#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "html-to-text": "^8.1.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.2.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.4",
    "nodemailer": "^6.7.2",
    "pug": "^3.0.2",
    "sharp": "^0.30.2",
    "slugify": "^1.6.5",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.7",
    "@babel/preset-env": "^7.16.11",
    "babel-loader": "^8.2.3",
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.3",
    "terser-webpack-plugin": "^5.3.1",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4",
    "webpack-merge": "^5.8.0"
  }
}
