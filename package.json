{
  "name": "natours-api",
  "version": "1.0.0",
  "description": "A simple application built with NodeJs and Express",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "cross-env NODE_ENV=development nodemon server.js -e js,pug,css,json",
    "start:prod": "cross-env NODE_ENV=production nodemon server.js -e js,pug,css,json",
    "import:tours:data": "node dev-data/data/import-data.js --import --tours",
    "wipe:tours:data": "node dev-data/data/import-data.js --wipe --tours",
    "import:users:data": "node dev-data/data/import-data.js --import --users",
    "wipe:users:data": "node dev-data/data/import-data.js --wipe --users",
    "import:reviews:data": "node dev-data/data/import-data.js --import --reviews",
    "wipe:reviews:data": "node dev-data/data/import-data.js --wipe --reviews",
    "import:all": "node dev-data/data/import-data.js --import --all",
    "wipe:all": "node dev-data/data/import-data.js --wipe --all"
  },
  "keywords": [
    "NodeJS",
    "and",
    "Express"
  ],
  "author": "Mark Njoroge",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "dotenv": "^12.0.4",
    "express": "^4.17.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^6.1.0",
    "helmet": "^5.0.1",
    "hpp": "^0.2.3",
    "html-to-text": "^8.1.0",
    "jsonwebtoken": "^8.5.1",
    "mapbox-gl": "^2.6.1",
    "mongoose": "^6.1.6",
    "morgan": "^1.10.0",
    "multer": "^1.4.4",
    "nodemailer": "^6.7.2",
    "pug": "^3.0.2",
    "sharp": "^0.30.1",
    "slugify": "^1.6.5",
    "stripe": "^8.202.0",
    "validator": "^13.7.0",
    "xss-clean": "^0.1.1"
  },
  "engines": {
    "node": ">= 10"
  }
}
