{
  "name": "natours-api",
  "version": "1.0.0",
  "description": "A simple application built with NodeJs and Express",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "cross-env NODE_ENV=development nodemon server.js",
    "start:prod": "cross-env NODE_ENV=production nodemon server.js",
    "import:tours:data": "node dev-data/data/import-json-data.js --import --tours",
    "wipe:tours:data": "node dev-data/data/import-json-data.js --wipe --tours",
    "import:users:data": "node dev-data/data/import-json-data.js --import --users",
    "wipe:users:data": "node dev-data/data/import-json-data.js --wipe --users",
    "import:reviews:data": "node dev-data/data/import-json-data.js --import --reviews",
    "wipe:reviews:data": "node dev-data/data/import-json-data.js --wipe --reviews",
    "import:data": "node dev-data/data/import-json-data.js --wipe --all",
    "wipe:data": "node dev-data/data/import-json-data.js --wipe --all"
  },
  "keywords": [
    "NodeJS",
    "and",
    "Express"
  ],
  "author": "Mark Njoroge",
  "license": "MIT",
  "dependencies": {
    "cross-env": "^7.0.3",
    "dotenv": "^11.0.0",
    "express": "^4.17.2",
    "mongoose": "^6.1.6",
    "slugify": "^1.6.5"
  },
  "devDependencies": {
    "morgan": "^1.10.0"
  },
  "engines": {
    "node": ">= 15"
  }
}
